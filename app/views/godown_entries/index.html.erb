<h1>Listing godown_entries</h1>


<%= link_to 'New Godown entry', new_godown_entry_path %>
<%= will_paginate @collection %>
<table>
  <tr>
    <th>Godown date</th>
    <th>Manufactory</th>
    <th>From warehouse</th>
    <th>To warehouse</th>
    <th>Info</th>
    <th>Total items</th>
    <th>Operator</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @collection.each do |godown_entry| %>
  <tr>
    <td><%= godown_entry.godown_date %></td>
    <td><%= godown_entry.manufactory_id %></td>
    <td><%= godown_entry.from_warehouse_id %></td>
    <td><%= godown_entry.to_warehouse_id %></td>
    <td><%= godown_entry.info %></td>
    <td><%= godown_entry.total_quantity %></td>
    <td><%= godown_entry.operator %></td>
    <td><%= link_to 'Show', godown_entry %></td>
    <td><%= link_to 'Edit', edit_godown_entry_path(godown_entry) %></td>
    <td><%= link_to 'Destroy', godown_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

    <%= will_paginate @collection %>
