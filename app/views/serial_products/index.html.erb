<h1>Listing serial_products</h1>
<%= link_to 'New Serial product', new_serial_product_path %>
<%= form_tag(batch_add_serial_products_path) do %>
  From: <%= text_field_tag :from_serial %>
  TO: <%= text_field_tag :to_serial %>
  <%= submit_tag %>
<% end %>

<%= will_paginate @collection %>
<table>
  <tr>
    <th>Serial number</th>
    <th>Product name</th>
    <th>Product model</th>
    <th>Title photo</th>
    <th>Production batch</th>
    <th>Product data type</th>
    <th>Product data</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @collection.each do |serial_product| %>
  <tr>
    <td><%= serial_product.serial_number %></td>
    <td><%= serial_product.product_name %></td>
    <td><%= serial_product.product_model %></td>
    <td><%= serial_product.title_photo_id %></td>
    <td><%= serial_product.production_batch_id %></td>
    <td><%= link_to 'obj', object_url(serial_product.product_data) %></td>
    <td><%= link_to 'Show', serial_product %></td>
    <td><%= link_to 'Edit', edit_serial_product_path(serial_product) %></td>
    <td><%= link_to 'Destroy', serial_product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>
<%= will_paginate @collection %>

<br />

