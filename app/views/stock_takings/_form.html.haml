= form_for(@object) do |f|
  - if @object.errors.any?
    #error_explanation
      %h2
        = pluralize(@object.errors.count, "error")
        prohibited this stock_taking from being saved:
      %ul
        - @object.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :warehouse_id
    %br/
    = f.number_field :warehouse_id
  .field
    = f.label :info
    %br/
    = f.text_field :info
  .field
    = f.label :status
    %br/
    = f.number_field :status
  .actions
    = f.submit
