<h1>Listing stock_takings</h1>

<table>
  <tr>
    <th>Warehouse</th>
    <th>Info</th>
    <th>Status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @collection.each do |stock_taking| %>
  <tr>
    <td><%= stock_taking.warehouse_id %></td>
    <td><%= stock_taking.info %></td>
    <td><%= stock_taking.status %></td>
    <td><%= link_to 'Show', stock_taking %></td>
    <td><%= link_to 'Edit', edit_stock_taking_path(stock_taking) %></td>
    <td><%= link_to 'Destroy', stock_taking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Stock taking', new_stock_taking_path %>
