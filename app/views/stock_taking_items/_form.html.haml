= form_for([@parent, @object]) do |f|
  - if @object.errors.any?
    #error_explanation
      %h2
        = pluralize(@object.errors.count, "error")
        prohibited this stock_taking_item from being saved:
      %ul
        - @object.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :stock_taking_id
    %br/
    = f.text_field :stock_taking_id, value: @parent.id
  .field
    = f.label :product_id
    %br/
    = f.text_field :product_id
  .field
    = f.label :full_name
    %br/
    = f.text_field :full_name
  .field
    = f.label :catalog
    %br/
    = f.text_field :catalog
  .field
    = f.label :quantity
    %br/
    = f.number_field :quantity
  .field
    = f.label :status
    %br/
    = f.number_field :status
  .actions
    = f.submit
