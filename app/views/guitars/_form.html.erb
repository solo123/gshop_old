<%= form_for([@parent, @object]) do |f| %>
  <% if @object.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@object.errors.count, "error") %> prohibited this guitar from being saved:</h2>

      <ul>
      <% @object.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="input-control text">
    Short Code:
    <%= f.text_field :short_code %>
  </div>
  <div class="input-control text">
    Brand:
    <%= f.text_field :brand %>
  </div>
  <div class="input-control text">
    Guitar type:
    <%= f.text_field :guitar_type %>
  </div>
  <div class="input-control text">
    Model:
    <%= f.text_field :model %>
  </div>
  <div class="input-control text">
    Price:
    <%= f.text_field :price %>
  </div>
  <div class="input-control text">
    Info:
    <%= f.text_field :info %>
  </div>
  <div class="field">
    <%= f.label :title_photo_id %><br />
    <%= f.number_field :title_photo_id %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
