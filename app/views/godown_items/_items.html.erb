<h1>Listing godown_items</h1>

<table>
  <tr>
    <th>Godown entry</th>
    <th>Product data type</th>
    <th>Product data</th>
    <th>Quantity</th>
    <th>Status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% items.each do |godown_item| %>
  <tr>
    <td><%= godown_item.godown_entry_id %></td>
    <td><%= godown_item.product_data_type %></td>
    <td><%= godown_item.product_data_id %></td>
    <td><%= godown_item.quantity %></td>
    <td><%= godown_item.status %></td>
    <td><%= link_to 'Show', godown_item %></td>
    <td><%= link_to 'Edit', edit_godown_item_path(godown_item) %></td>
    <td><%= link_to 'Destroy', godown_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />


