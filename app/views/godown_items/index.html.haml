%h1 Listing godown_items
%table
  %tr
    %th Godown entry
    %th Product data type
    %th Product data
    %th Quantity
    %th Status
    %th
    %th
    %th
  - @godown_items.each do |godown_item|
    %tr
      %td= godown_item.godown_entry_id
      %td= godown_item.product_data_type
      %td= godown_item.product_data_id
      %td= godown_item.quantity
      %td= godown_item.status
      %td= link_to 'Show', godown_item
      %td= link_to 'Edit', edit_godown_item_path(godown_item)
      %td= link_to 'Destroy', godown_item, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Godown item', new_godown_item_path
