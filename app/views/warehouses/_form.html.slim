= form_for(@object) do |f|
  - if @object.errors.any?
    #error_explanation
      h2
        = pluralize(@object.errors.count, "error")
        | prohibited this warehouse from being saved:
      ul
        - @object.errors.full_messages.each do |msg|
          li= msg
  .field
    = f.label :name
    br/
    = f.text_field :name
  .field
    = f.label :pre_code
    br/
    = f.text_field :pre_code
  .field
    = f.label :status
    br/
    = f.number_field :status
  .actions
    = f.submit
